{
  "name": "angular-spa",
  "version": "0.1.5",
  "description": "Utilities for angular 2 applications backed by generator-spring-spa project",
  "main": "index.js",
  "scripts": {
    "lint": "tslint ./src/**/*.ts -t verbose",
    "lint:fix": "tslint ./src/**/*.ts --fix",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "compile": "tsc",
    "watch": "tsc --watch",
    "build": "npm run compile",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "webdriver:update": "webdriver-manager update",
    "copyfiles": "cp -rf package.json dist && cp -rf README.md dist && cp -rf LICENSE dist",
    "dist": "npm run build && npm run copyfiles && cd dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atende/angular-spa.git"
  },
  "keywords": [
    "angular2",
    "utils",
    "ui"
  ],
  "author": "Giovanni Silva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atende/angular-angular-spa/issues"
  },
  "homepage": "https://github.com/atende/angular-angular-spa#readme",
  "dependencies": {
    "@angular/common": ">=2.0.0 <=3.0.0",
    "@angular/core": ">=2.0.0 <=3.0.0",
    "@angular/http": ">=2.0.0 <=3.0.0",
    "@angular/platform-browser": ">=2.0.0 <=3.0.0",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "@angular/compiler": ">=2.0.0 <=3.0.0",
    "@angular/platform-browser-dynamic": ">=2.0.0 <=3.0.0",
    "@types/jasmine": "^2.5.36",
    "@types/node": "^6.0.46",
    "@types/selenium-webdriver": "^2.53.33",
    "angular-in-memory-web-api": "~0.1.13",
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "0.0.26",
    "protractor": "4.0.9",
    "reflect-metadata": "^0.1.8",
    "systemjs": "0.19.40",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3"
  }
}
